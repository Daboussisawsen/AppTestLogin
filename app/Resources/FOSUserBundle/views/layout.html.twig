<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <!-- Bootstrap CSS -->
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">

    </head>
    <body>
        <header class="navbar navbar-light" style="background-color: #e3f2fd;">
            {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                    <a class="ml-md-auto" href="{{ path('fos_user_security_logout') }}">
                        {{ 'Deconnexion'|trans({}, 'FOSUserBundle') }}
                    </a>
            {% else %}
                <a class="ml-md-auto" href="{{ path('fos_user_registration_register') }}">
                        {{ "Inscription"|trans({}, "FOSUserBundle") }}
                    </a>
            {% endif %}
            
           
        </header>
        <div class="container-fluid">
            <div class="row">
                <div class="col-12 col-md-3">
                    
                </div>
                <div class="col-12 col-md-6 bg-light" style="padding-top: 25px;">

                    {% block fos_user_content %}
                    {% endblock fos_user_content %}
                </div>
                <div class="col-12 col-md-3">
                    
                </div>
        </div>
    </body>
</html>
